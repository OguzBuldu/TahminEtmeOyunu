            List<string> takım = new List<string> { "Fenerbahçe", "Galatasaray", "Beşiktaş", "Sivasspor", "Trabzonspor", "Adanaspor" };
            int sira = 1;
            Random skorlar = new Random();


            foreach (string takımlar in takım)
            {

                Console.WriteLine($"{sira}. {takımlar}");
                sira++;
            }



            int skor = skorlar.Next(10);
            int skor2 = skorlar.Next(10);

            while (true)
            {
                Console.WriteLine("Lütfen Yukardan Bir Kulüp Seçiniz.");
                int kulüp_seçim = int.Parse(Console.ReadLine());

